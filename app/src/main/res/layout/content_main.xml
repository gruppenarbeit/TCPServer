<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:screenOrientation="portrait"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="group_project.test001.MainActivity"
    tools:showIn="@layout/activity_main"><![CDATA[




        android:layout_centerHorizontal="true" />












    ]]>

    <TextView
        android:text="Socket"
        android:layout_width="match_parent"
        android:id="@+id/Sockettext"
        tools:text="Socket"
        android:layout_marginTop="26dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_height="250dp" />

    <Button
        android:text="Scan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/send_Scan"
        android:layout_marginTop="45dp"
        android:background="@color/accent_material_light"
        android:layout_below="@+id/Sockettext"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Live"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/send_live"
        android:background="@color/accent_material_light"
        android:layout_alignBaseline="@+id/send_detail"
        android:layout_alignBottom="@+id/send_detail"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="Calibrate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/send_calitrigger"
        android:background="@color/accent_material_light"
        android:layout_alignBaseline="@+id/stop_live"
        android:layout_alignBottom="@+id/stop_live"
        android:layout_alignParentStart="true" />

    <Button
        android:text="LiveStop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/stop_live"
        android:background="@color/accent_material_light"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/send_live"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="Detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/send_detail"
        android:background="@color/accent_material_light"
        android:layout_alignBaseline="@+id/send_Scan"
        android:layout_alignBottom="@+id/send_Scan"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="StopService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/stop_service"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
